CREATE TABLE Transactions (

transaction_Id INT (10) AUTO_INCREMENT,
user_Id;
uransaction_Type ('INCOMING', 'OUTGOING');
transaction_Date
transaction_Amount;
transaction_Fee;
recipient_client_Id INT,                                  
recipient_Iban VARCHAR(),                              
recipient_bic VARCHAR(),                              
recipient_first_name VARCHAR(),                    
recipient_last_name VARCHAR(),        
user_balance_before DECIMAL(, ) ,              
user_balance_after DECIMAL(, ) ,             
transfer_purpose TEXT,

PRIMARY KEY (id)
);
